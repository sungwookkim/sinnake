<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>properties</comment>

	<!-- 서버 관련 프로퍼티 내용 시작 -->
    
     <!-- 서버 유형(local, dev, stage, real) -->
    <entry key="type.propertie">local</entry>
    <!-- DB 종류 -->
 	<entry key="dbType.propertie">mysql</entry>
 	<!-- 기본 언어 --> 
 	<entry key="locale.propertie">ko</entry> 
 	
 	<entry key="devPropFile.propertie">classpath:properties/dev/dev_${serverType}_properties</entry>
 	<!-- 서버유형별 프로퍼티 파일 -->
 	<entry key="serverPropFile.propertie">properties/server/server_${serverType}_properties.xml</entry> 
 	<!-- 서버유형별 DB 종류 프로퍼티 파일-->
 	<entry key="dbPropFile.propertie">properties/db/${serverDb}/db_${serverType}_${serverDb}_properties.xml</entry>
 	 
	<!-- 서버 관련 프로퍼티 내용 끝 -->
</properties>
